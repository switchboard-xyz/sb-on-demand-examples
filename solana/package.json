{
  "name": "@switchboard-xyz/on-demand-feed-example",
  "version": "1.0.0",
  "description": "Example implementation of Switchboard On-Demand oracles with 90% lower costs and sub-second latency",
  "repository": {
    "type": "git",
    "url": "https://github.com/switchboard-xyz/sb-on-demand-examples"
  },
  "keywords": [
    "solana",
    "oracle",
    "switchboard",
    "defi",
    "blockchain",
    "price-feed",
    "on-demand"
  ],
  "author": "Switchboard",
  "license": "MIT",
  "main": "scripts/index.ts",
  "scripts": {
    "start": "ts-node examples/feeds/basic/managedUpdate.ts",
    "fetch-prices": "ts-node examples/feeds/basic/managedUpdate.ts",
    "feeds:managed": "ts-node examples/feeds/basic/managedUpdate.ts",
    "feeds:advanced": "ts-node examples/feeds/advanced/runUpdate.ts",
    "feeds:legacy": "ts-node examples/feeds/legacy/runFeed.ts",
    "benchmark": "NODE_NO_WARNINGS=1 ts-node examples/benchmarks/benchmark.ts",
    "benchmark:cu": "NODE_NO_WARNINGS=1 ts-node examples/benchmarks/benchmarkCU.ts",
    "stream:surge": "ts-node surge/runSurge.ts",
    "stream:crossbar": "ts-node ../common/streaming/crossbarStream.ts",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "build": "cargo build-sbf",
    "deploy": "solana program deploy target/deploy/advanced_oracle_example.so",
    "test": "cargo test",
    "test:litesvm": "cargo test --test litesvm_oracle_test",
    "clippy": "cargo clippy",
    "fmt": "cargo fmt"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@faremeter/fetch": "^0.8.0",
    "@faremeter/payment-solana": "^0.8.0",
    "@faremeter/types": "^0.9.0",
    "@faremeter/wallet-solana": "^0.8.0",
    "@faremeter/x-solana-settlement": "^0.2.1",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.0",
    "@switchboard-xyz/common": "^5.1.0",
    "@switchboard-xyz/on-demand": "^3.1.1",
    "@switchboard-xyz/x402-utils": "0.2.1",
    "assert": "^2.1.0",
    "axios": "^1.8.4",
    "big.js": "^6.2.2",
    "bigint-buffer": "^1.1.5",
    "bs58": "^5.0.0",
    "client": "link:@faremeter/types/client",
    "exact": "link:@faremeter/payment-solana/exact",
    "path": "^0.12.7",
    "solana": "link:@faremeter/info/solana",
    "web3": "^4.16.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^9.1.1",
    "@types/node": "^22.13.10",
    "@types/yargs": "^17.0.33",
    "chai": "^4.5.0",
    "mocha": "^9.2.2",
    "prettier": "^2.8.8",
    "ts-mocha": "^10.1.0",
    "typescript": "^4.9.5"
  },
  "pnpm": {
    "patchedDependencies": {
      "@faremeter/payment-solana@0.10.0": "patches/@faremeter__payment-solana@0.10.0.patch",
      "@faremeter/payment-solana@0.8.0": "patches/@faremeter__payment-solana@0.8.0.patch"
    }
  }
}
