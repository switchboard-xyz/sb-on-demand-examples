{
  "name": "evm-switchboard-examples",
  "version": "2.0.0",
  "description": "Comprehensive Switchboard On-Demand examples for EVM chains",
  "type": "module",
  "scripts": {
    "example": "bun scripts/run.ts",
    "update": "bun examples/updateFeed.ts",
    "randomness": "bun examples/requestRandomness.ts",
    "surge-convert": "bun examples/surgeToEvmConversion.ts",
    "build": "forge build",
    "test": "forge test",
    "deploy": "forge script script/DeploySwitchboardPriceConsumer.s.sol:DeploySwitchboardPriceConsumer --broadcast -vvvv",
    "deploy:monad-testnet": "forge script script/DeploySwitchboardPriceConsumer.s.sol:DeploySwitchboardPriceConsumer --rpc-url https://testnet-rpc.monad.xyz --broadcast -vvvv",
    "deploy:monad-mainnet": "forge script script/DeploySwitchboardPriceConsumer.s.sol:DeploySwitchboardPriceConsumer --rpc-url $MONAD_RPC_URL --broadcast -vvvv",
    "deploy:randomness": "forge script script/DeployRandomnessConsumer.s.sol:DeployRandomnessConsumer --rpc-url https://testnet-rpc.monad.xyz --broadcast -vvvv",
    "verify": "forge verify-contract --chain-id $CHAIN_ID --watch $CONTRACT_ADDRESS src/SwitchboardPriceConsumer.sol:SwitchboardPriceConsumer"
  },
  "keywords": [
    "switchboard",
    "oracle",
    "evm",
    "ethereum",
    "monad",
    "arbitrum",
    "defi",
    "price-feeds"
  ],
  "author": "Switchboard Labs",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@switchboard-xyz/common": "^5.5.0",
    "@switchboard-xyz/on-demand": "^3.7.3",
    "@switchboard-xyz/on-demand-solidity": "1.0.1-alpha0.0.0",
    "@types/yargs": "^17.0.33",
    "ethers": "^6.13.1",
    "forge-std": "github:foundry-rs/forge-std#v1.9.4",
    "qrcode-terminal": "^0.12.0",
    "yargs": "^18.0.0"
  }
}